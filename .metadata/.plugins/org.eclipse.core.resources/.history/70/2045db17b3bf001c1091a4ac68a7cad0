import java.util.*;

public class ex1 {

   public static void main(String[] args) {

	   int[] ballArr = {1,2,3,4,5,6,7,8,9};
	   int[] ball3 = new int [3];
	   
	   for(int i= 0; i<ballArr.length; i++) {
		   int j=(int)((Math.random()*ballArr.length)*9);
		   int tmp = 0;
		   tmp = ballArr[i];
		   ballArr[i] = ballArr[j];
		   ballArr[j] = tmp;
		   
	   }
	   for (int i = 0; i<ball3.length; i++) {
		   System.out.println(ball3[i]);
	   }
   }
}
